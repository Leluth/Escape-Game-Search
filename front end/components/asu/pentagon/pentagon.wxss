/* components/asu/pentagon/pentagon.wxss */

pentagon {
  position: relative;
  width: 200px;
  height: 200px;
  display: block;
}

.polygon {
  position: absolute;
  top: 0;
  left: 0;
  width: 70%;
  height: 70%;
  margin: 15%;
  background: white;
  transition: all 2s;
}

canvas {
  width: 100%;
  height: 100%;
}
